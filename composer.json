{
    "name": "mindy/orm-slug-field",
    "homepage": "https://github.com/MindyPHP/OrmSlugField",
    "license": "MIT",
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Mindy\\Orm\\Fields\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mindy\\Orm\\Fields\\Tests\\": "./Tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "mindy/orm": ">=4.0",
        "cocur/slugify": ">=3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.5",
        "mindy/orm-nested-set": ">=4.0"
    },
    "suggested": {
        "mindy/orm-nested-set": "Nested set for Mindy Orm"
    },
    "authors": [
        {
            "name": "Maxim Falaleev",
            "email": "max@studio107.ru",
            "homepage": "http://studio107.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/MindyPHP/Mindy/issues?state=open"
    }
}
